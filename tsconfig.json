{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@models/*": ["src/models/*"]
    },
    "declaration": true,
    "lib": ["es2015", "dom"],
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "skipLibCheck": true,
    "target": "ES6",
    "moduleResolution": "node"
  }
}
