{
  "name": "@aut-protocol/sdk-biconomy",
  "version": "0.0.7",
  "description": "It a biconomy SDK wrapper library to help with the issues coming from installing with webpack 5 polyfills.",
  "keywords": [
    "sdk",
    "aut",
    "typescript",
    "aut-protocol",
    "web3",
    "DAO"
  ],
  "license": "MIT",
  "author": "Aut Labs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
    "prettier-format": "prettier --config .prettierrc.json 'src/**/*.ts' --write",
    "watch": "webpack --build"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "tsc-alias": "^1.7.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4",
    "@biconomy/mexa": "^3.0.4",
    "@webpack-cli/generators": "^2.5.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "dateformat": "^5.0.2",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.1",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "request": "^2.88.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-loader": "^9.4.1",
